{
  "name": "homebridge-simplisafe3",
  "version": "1.10.13",
  "description": "A complete Homebridge plugin for the SimpliSafe 3 security system",
  "author": "NiccolÃ² Zapponi",
  "bin": {
    "homebridge-simplisafe3": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/homebridge-simplisafe3/homebridge-simplisafe3/issues"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "0.0.19",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.28.0",
    "axios-retry": "^3.2.4",
    "core-js": "^3.19.3",
    "ffmpeg-for-homebridge": "^0.1.4",
    "ip": "^1.1.5",
    "is-docker": "^2.2.1",
    "jpeg-extract": "^3.0.1",
    "json-schema": ">=0.4.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.12.0",
    "eslint-plugin-babel": "^5.3.1"
  },
  "engines": {
    "homebridge": ">=1.3.5 || ^2.0.0-beta.0",
    "node": ">=14.13.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "babel"
    ],
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "babel/semi": 1
    }
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/simplisafe.js",
    "/accessories",
    "/commands",
    "/lib",
    "/homebridge-ui",
    "/config.schema.json",
    "/images",
    "/README.md"
  ],
  "homepage": "https://github.com/homebridge-simplisafe3/homebridge-simplisafe3#readme",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "homekit",
    "security",
    "simplisafe"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./commands",
    "bin": "homebridge-simplisafe3",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "git+https://github.com/homebridge-simplisafe3/homebridge-simplisafe3.git",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist && cp -r package.json config.schema.json homebridge-ui bin images README.md dist/",
    "deploy": "npm publish dist/",
    "release": "./scripts/release.sh",
    "start": "nodemon --exec babel-node src/index.js",
    "test": "echo NO TESTS"
  }
}
